																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="Name">
																						<xsd:annotation>
																							<xsd:documentation>Rule Name.
Example: Acceptable Order Item Price Variance</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																				</xsd:extension>
																			</xsd:simpleContent>
																		</xsd:complexType>
																	</xsd:element>
																	<xsd:element name="Amount" minOccurs="0">
																		<xsd:annotation>
																			<xsd:documentation>Price Variance Amount and/or Percentage.
Example: 20 USD, 2 Percent</xsd:documentation>
																		</xsd:annotation>
																		<xsd:complexType>
																			<xsd:sequence>
																				<xsd:element ref="AmountOrPercentageGroup"/>
																			</xsd:sequence>
																		</xsd:complexType>
																	</xsd:element>
																	<xsd:element name="Associations" minOccurs="0">
																		<xsd:annotation>
																			<xsd:documentation>Association(s).</xsd:documentation>
																		</xsd:annotation>
																		<xsd:complexType>
																			<xsd:sequence>
